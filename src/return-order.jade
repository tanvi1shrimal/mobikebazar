						a(href='#').button.button--primary-white.button--lg logout
					h3.orders-title.text-center.visible-xs my orders
					.tabs-content.order-tabs
						.tab-content.orders-tab-content
							.order
								//START: Order on Query
								.order__list
									.order__placed-details
										.order__number
											span.order__status 
											| OD19372987263438
											span.order__price.hidden-xs Total Rs 3380
										//.order__placed Order Placed on Thursday, Nov 09, 2016
									.order__product
										p.order-info Select the item you want to return
										.order__product-details.pull-left
											.form-group
												label.control.control--checkbox(for='product-1')
													img(src="img/order-1.png", alt="").order__image
													input(type='checkbox', name='checkbox')#product-1
													.control__indicator
											//img(src="img/order-1.png", alt="").order__image
										h5.product-name BAJAJ / AVENGER 180 / 2009 - Chain part
									.order__product
										.order__product-details.pull-left
											.form-group
												label.control.control--checkbox(for='product-2')
													img(src="img/order-1.png", alt="").order__image
													input(type='checkbox', name='checkbox')#product-2
													.control__indicator
											//img(src="img/order-1.png", alt="").order__image
										h5.product-name BAJAJ / AVENGER 180 / 2009 - Chain part
								
							.col-sm-12.buttons-container
								
								a(href='my-orders.html').button.button--primary-white.button--lg cancel
								a(href='return-order-1.html').button.button--primary-white.button--lg.button--next next
										

