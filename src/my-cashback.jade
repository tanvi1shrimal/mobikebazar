	//START: Popup Bike Registarion
	#register-bike-final-reg.modal.bike-register.modal--fullxs.fade(role='dialog')
		.modal-dialog.modal__dialog
			.modal-content.modal__content
				.modal-header
					button(type='button' data-dismiss='modal').close.close.close--closebike
						i.mb-icon-close.modal__close-icon
					h4.modal-title.modal__title Register your vehicle
				.modal-body.modal-scrollbar.text-center.clearfix
					p.bike-register__info We would need your motorcycle’s
					form(role='form').col-sm-6.col-sm-offset-3.bike-register__form
						.row
							.col-sm-12
								.form-group
									span.input.input--hoshi
										input(type='text').input__field.input__field--hoshi
										label(for='number').input__label.input__label--hoshi-color-1.input__label--hoshi
											span.input__label-content--hoshi.input__label-content
												| Registration Number
					p.or-content.clear or

					form(role='form').col-sm-6.col-sm-offset-3.bike-register__form
						.row
							.col-sm-12
								.form-group
									span.input.input--hoshi
