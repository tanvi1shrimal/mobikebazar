										label(for='number').input__label.input__label--hoshi-color-1.input__label--hoshi
											span.input__label-content--hoshi.input__label-content Engine Number
					.text-center.clear
						i.info Why should i provide these details ?
						a(href='#' data-dismiss='modal').button.button--primary-white.button--lg Finish registration
	//START: Modal: Add new Address
	#addNewAdress.modal.fade.modal--fullxs(role='dialog')
		.modal-dialog(role='document')
			.modal-content
				.modal-header
					button.close(type='button', data-dismiss='modal', aria-label='Close')
						i.mb-icon-close.modal__close-icon
					h4.modal-title add new address
				.modal-body
					form.login-form
						.row
							.col-sm-6
								.form-group
									span.input.input--hoshi
										input#pin-code.input__field.input__field--hoshi(type='text')
										label.input__label.input__label--hoshi.input__label--hoshi-color-1(for='pin-code')
